KAHA;;AAKA,QAAM,KAAI,GAAI,SAAR,KAAQ,CAAC,EAAD,EAAK,IAAL,EAAc;AAC1B,MAAA,IAAI,CAAC,EAAL,CAAQ,EAAR,EAAY;AACV,QAAA,OAAO,EAAE,CADC;AAEV,QAAA,CAAC,EAAE,CAFO;AAGV,QAAA,QAAQ,EAAE,GAHA;AAIV,QAAA,UAAU,EAAE,IAJF;AAKV,QAAA,KAAK,EAAE,EAAE,CAAC,OAAH,CAAW,KAAX,GAAmB;AALhB,OAAZ;AAOF,KARA;;AAUA,WAAO;AACL,MAAA,KAAK,EAAL,KADK;AAEL,MAAA,WAAW,EAAX,WAFK;AAGL,MAAA,KAAI,EAAJ;AAHK,KAAP;AAKF;AA7Ba,CAAf","sourcesContent":["<template>\n  <div class=\"contact\">\n    <h1>Contact</h1>\n    <transition-group\n      appear \n      tag=\"ul\"\n      @before-enter=\"beforeEnter\"\n      @enter=\"enter\"\n    >\n      <li v-for=\"(icon, index) in icons\" :key=\"icon.name\" :data-index=\"index\">\n        <span class=\"material-icons\">{{ icon.name }}</span>\n        <div>{{ icon.text }}</div>\n      </li>\n    </transition-group>\n  </div>\n</template>\n\n<script>\nimport { ref } from 'vue'\nimport gsap from 'gsap'\n\nexport default {\n  setup() {\n    const icons = ref([\n      { name: 'alternate_email', text: 'by email'},\n      { name: 'local_phone', text: 'by phone'},\n      { name: 'local_post_office', text: 'by post'},\n      { name: 'local_fire_department', text: 'by smoke signal'},\n    ])\n\n    const beforeEnter = (el) => {\n      el.style.opacity = 0;\n      el.style.transform = 'translateY(100px)'\n    }\n\n    const enter = (el, done) => {\n      gsap.to(el, {\n        opacity: 1,\n        y: 0,\n        duration: 0.8,\n        onComplete: done,\n        delay: el.dataset.index * 0.2\n      })\n    }\n\n    return { \n      icons, \n      beforeEnter, \n      enter \n    }\n  }\n}\n</script>\n\n<style>\n  .contact ul {\n    padding: 0;\n    display: grid;\n    grid-template-columns: 1fr 1fr;\n    grid-gap: 20px;\n    max-width: 400px;\n    margin: 60px auto;\n  }\n  .contact li {\n    list-style-type: none;\n    background: white;\n    padding: 30px;\n    border-radius: 10px;\n    box-shadow: 1px 3px 5px rgba(0,0,0,0.1);\n    cursor: pointer;\n    line-height: 1.5em;\n  }\n</style>"],"sourceRoot":""}]}