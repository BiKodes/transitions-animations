   mode=\"out-in\"\n            @before-enter=\"beforeEnter\"\n            @after-enter=\"afterEnter\"\n            @enter=\"enter\"\n            @before-leave=\"beforeLeave\"\n            @after-leave=\"afterLeave\"\n            @leave=\"leave\"\n            >\n            <button\n            class=\"btn btn-primary\"\n            @click.prevent=\"onShowButton()\"\n            v-if=\"!showOnButton\">\n            Off\n            </button>\n\n             <button\n            class=\"btn btn-primary\"\n            v-else\n            @click.prevent=\"onShowButton()\">\n            On\n            </button>\n        </transition>\n        \n    </div>\n</template>\n\n<script> \nexport default {\n    name: 'AnimateButton',\n\n    data() {\n        return {\n            showOnButton: true,\n        }\n    },\n\n    methods: {\n        onShowButton(){\n            this.showOnButton = !this.showOnButton;\n        },\n\n        beforeEnter(){\n          console.log('before Enter')  \n        },\n\n        afterEnter(){\n          console.log('after Enter')   \n        },\n\n        enter(){\n          console.log('enter')   \n        },\n\n        beforeLeave(){\n            console.log('before Leave')\n        },\n\n        afterLeave(){\n            console.log('after Leave')\n        },\n\n        leave(){\n            console.log('leave')\n        }\n\n    },\n};\n</script>\n\n<style scoped>\n\n.button-enter-from,\n.button-leave-to {\n    opacity: 0;\n    transform: translateX(-30px);\n}\n\n.button-enter-to,\n.button-leave-from {\n    opacity: 1;\n    transform: translateX(0px);\n}\n\n.button-enter-active,\n.button-leave-active {\n    transition: all 1s;\n}\n\n</style>"]}]}