button>\n        </transition>\n        \n    </div>\n</template>\n\n<script> \nexport default {\n    name: 'AnimateButton',\n\n    data() {\n        return {\n            showOnButton: true,\n            enterInterval: '',\n            leaveInterval: ''\n        };\n    },\n\n    methods: {\n        onShowButton(){\n            this.showOnButton = !this.showOnButton;\n        },\n\n        beforeEnter(el){\n            console.log('before enter')\n            el.style.opacity = 0; \n        },\n\n        enterCancel(){\n\n        },\n\n        leaveCancel() {\n\n        },\n\n        afterEnter(){\n          console.log('after Enter')   \n        },\n\n        enter(el, done){\n            console.log('enter');\n            let step = 0.01\n            let opacity = 0;\n\n            this.enterInterval = setInterval(() => {\n                console.log('enter interval running');\n                opacity += step;\n                el.style.opacity = opacity;\n\n                if (opacity >= 1) {\n                    clearInterval(interval);\n                    done();\n                } \n            }, 10);\n        },\n\n        beforeLeave(el){\n            console.log('before leave')\n            el.style.opacity = 1; \n        },\n\n        afterLeave(){\n            console.log('after Leave')\n        },\n\n        leave(el, done){\n            console.log('leave');\n            let step = 0.01\n            let opacity = 1;\n\n            const interval = setInterval(() => {\n                console.log('leave interval running');\n                opacity -= step;\n                el.style.opacity = opacity;\n\n                if (opacity <= 0) {\n                    clearInterval(interval);\n                    done();\n                } \n            }, 10);\n        }\n\n    },\n};\n</script>\n\n<style scoped>\n/* \n.button-enter-from,\n.button-leave-to {\n    opacity: 0;\n    transform: translateX(-30px);\n}\n\n.button-enter-to,\n.button-leave-from {\n    opacity: 1;\n    transform: translateX(0px);\n}\n\n.button-enter-active,\n.button-leave-active {\n    transition: all 1s;\n} */\n \n</style>"],"sourceRoot":""}]}