IACJ,CAAC;AACL,CAAC","file":"/home/stevo/dev/vue-transitions-animation/extensive-examples/javascript-hooks-events/src/components/AnimateButton.vue","sourceRoot":"","sourcesContent":["<template>\n    <div class=\"text-primary py-4 text-center\" style=\"border: 1px solid #000\">\n\n        <transition name=\"button\" mode=\"out-in\">\n            <button\n            class=\"btn btn-primary\"\n            @click.prevent=\"onShowButton()\"\n            v-if=\"!showOnButton\">\n            Off\n            </button>\n\n             <button\n            class=\"btn btn-primary\"\n            v-else\n            @click.prevent=\"onShowButton()\">\n            On\n            </button>\n        </transition>\n        \n    </div>\n</template>\n\n<script>\nexport default {\n    name: 'AnimateButton',\n\n    data() {\n        return {\n            showOnButton: true,\n        }\n    },\n\n    methods: {\n        onShowButton(){\n            this.showOnButton = !this.showOnButton;\n        }\n    },\n};\n</script>\n\n<style scoped>\n\n.button-enter-from,\n.button-leave-to {\n    opacity: 0;\n    transform: translateX(-30px);\n}\n\n.button-enter-to,\n.button-leave-from {\n    opacity: 1;\n    transform: translateX(0px);\n}\n\n.button-enter-active,\n.button-leave-active {\n    transition: all 1s;\n}\n\n</style>"]}]}